{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#2f6a7c",
    "activityBar.activeBorder": "#422c74",
    "activityBar.background": "#2f6a7c",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#422c74",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#2f6a7c",
    "statusBar.background": "#2f6a7c",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2f767c",
    "statusBarItem.remoteBackground": "#2f6a7c",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#2f6a7c",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#2f6a7c",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
